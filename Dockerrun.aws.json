{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
	{
	  "name": "php-fpm",
	  "image": "nickwshaw/clocks-php",
	  "hostname": "php-fpm",
	  "essential": true,
	  "memory": 256
	},
	{
	  "name": "nginx",
	  "image": "nickwshaw/clocks-nginx",
	  "hostname": "nginx",
	  "essential": true,
	  "memory": 128,
	  "portMappings": [
		{
		  "hostPort": 80,
		  "containerPort": 80
		}
	  ],
	  "links": [
		"php-fpm"
	  ]
	}
  ]
}
